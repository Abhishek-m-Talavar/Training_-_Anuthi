    "print(\"\\nValue counts before encoding:\")\n",
    "print(df['diagnosis'].value_counts())\n",
    "\n",
    "# Initialize the LabelEncoder\n",
    "label = LabelEncoder()\n",
    "\n",
    "# Apply label encoding to 'diagnosis' column\n",
    "data_label = label.fit_transform(df['diagnosis'])\n",

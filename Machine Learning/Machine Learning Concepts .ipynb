    "# Import required libraries\n",
    "import pandas as pd\n",
    "from sklearn.model_selection import train_test_split\n",
    "from sklearn.preprocessing import StandardScaler\n",
    "\n",
    "# Load the dataset\n",
    "df = pd.read_csv('diabetes.csv')\n",
    "\n",
    "# Display the first few rows of the dataset\n",
    "print(\"🔹 First 5 rows of the dataset:\")\n",
    "print(df.head())\n",
    "\n",
    "# Separate features (X) and target (Y)\n",
    "X = df.drop(columns='Outcome', axis=1)\n",
    "Y = df['Outcome']\n",
    "\n",
    "# Display shape of features and target\n",
    "print(\"\\n🔹 Shape of original dataset:\")\n",
    "print(\"Features (X):\", X.shape)\n",
    "print(\"Target (Y):\", Y.shape)\n",
    "\n",
    "# Display feature names\n",
    "print(\"\\n🔹 Feature columns:\")\n",
    "print(X.columns.tolist())\n",
    "\n",
    "# Display target value counts\n",
    "print(\"\\n🔹 Target distribution:\")\n",
    "print(Y.value_counts())\n",
    "\n",
    "# Initialize and fit the StandardScaler on the entire feature set\n",
    "scaler = StandardScaler()\n",
    "scaler.fit(X)\n",
    "\n",
    "# Transform the feature data using the scaler\n",
    "X_scaled = scaler.transform(X)\n",
    "\n",
    "# Convert scaled data back to DataFrame for easier viewing (optional)\n",
    "X_scaled_df = pd.DataFrame(X_scaled, columns=X.columns)\n",
    "\n",

   "source": [
    "# -------------------------------\n",
    "# 📦 Step 1: Import Dependencies\n",
    "# -------------------------------\n",
    "import pandas as pd\n",
    "from imblearn.under_sampling import RandomUnderSampler\n",
    "from imblearn.over_sampling import SMOTE\n",
    "\n",
    "# -------------------------------\n",
    "# 📂 Step 2: Load the Dataset\n",
    "# -------------------------------\n",
    "credit_card_data = pd.read_csv(\"credit_data.csv\")\n",
    "\n",
    "# View top and bottom rows\n",
    "print(\"Top 5 rows:\")\n",
    "print(credit_card_data.head())\n",
    "print(\"\\nBottom 5 rows:\")\n",
    "print(credit_card_data.tail())\n",
    "\n",
    "# -------------------------------\n",
    "# 📊 Step 3: Check Class Distribution\n",
    "# -------------------------------\n",
    "# Class: 0 → Legit transactions, 1 → Fraudulent transactions\n",
    "print(\"\\nClass distribution:\")\n",
    "print(credit_card_data['Class'].value_counts())\n",
    "\n",
    "# 🔎 Observation: Data is highly imbalanced (~90% legit)\n",
    "\n",
    "# -------------------------------\n",
    "# 🔄 Step 4: Separate Legit & Fraud\n",
    "# -------------------------------\n",
    "legit = credit_card_data[credit_card_data.Class == 0]\n",
    "fraud = credit_card_data[credit_card_data.Class == 1]\n",
    "\n",
    "print(\"\\nLegit shape:\", legit.shape)\n",
    "print(\"Fraud shape:\", fraud.shape)\n",
    "\n",
    "# -------------------------------\n",
    "# ✂️ Step 5: Manual Undersampling (Optional Example)\n",
    "# -------------------------------\n",
    "# Take 492 samples from legit to match fraud count (492)\n",
    "legit_sample = legit.sample(n=492)\n",
    "print(\"\\nManually undersampled legit shape:\", legit_sample.shape)\n",
    "\n",
    "# -------------------------------\n",
    "# 🧾 Step 6: Separate Features (X) and Target (Y)\n",
    "# -------------------------------\n",
    "X = credit_card_data.drop('Class', axis=1)\n",
    "Y = credit_card_data['Class']\n",
    "\n",
    "# -------------------------------\n",
    "# 🔁 Step 7: Apply Random Undersampling\n",
    "# -------------------------------\n",
    "rus = RandomUnderSampler()\n",
    "X_res, Y_res = rus.fit_resample(X, Y)\n",
    "\n",
    "print(\"\\nAfter RandomUnderSampler:\")\n",
    "print(\"X_res shape:\", X_res.shape)\n",
    "print(\"Y_res distribution:\\n\", pd.Series(Y_res).value_counts())\n",
    "\n",
    "# -------------------------------\n",
    "# 🔁 Step 8: Apply SMOTE Oversampling\n",
    "# -------------------------------\n",
    "smt = SMOTE()\n",
    "X_over, Y_over = smt.fit_resample(X, Y)\n",
    "\n",
    "print(\"\\nAfter SMOTE Oversampling:\")\n",
    "print(\"X_over shape:\", X_over.shape)\n",
    "print(\"Y_over distribution:\\n\", pd.Series(Y_over).value_counts())\n"
   ]
  },

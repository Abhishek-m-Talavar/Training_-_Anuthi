    "# Import necessary libraries\n",
    "import pandas as pd\n",
    "from sklearn.model_selection import train_test_split\n",
    "from sklearn.preprocessing import StandardScaler\n",
    "\n",
    "# Load dataset from CSV file\n",
    "df = pd.read_csv('autompg.csv')\n",
    "\n",
    "# Display first few rows (optional during debugging)\n",
    "print(\"First few rows of the dataset:\")\n",
    "print(df.head())\n",
    "\n",
    "# Select specific columns for features (X) and target (Y)\n",
    "# X: displacement (index 2) and weight (index 4)\n",
    "# Y: acceleration (index 5)\n",
    "X = df.iloc[:, [2, 4]]\n",
    "Y = df.iloc[:, 5]\n",
    "\n",
    "# Optional: Display selected columns\n",
    "print(\"\\nFeatures (X):\")\n",
    "print(X.head())\n",
    "\n",
    "print(\"\\nTarget (Y):\")\n",
    "print(Y.head())\n",
    "\n",
    "# Split the dataset into training and testing sets (80% train, 20% test)\n",
    "X_train, X_test, Y_train, Y_test = train_test_split(X, Y, test_size=0.20, random_state=1)\n",
    "\n",
    "# Standardize the training features\n",
    "scaler = StandardScaler()\n",
    "scaler.fit(X_train)\n",
    "\n",
    "# Print the mean and scale (std deviation) used for standardization\n",
    "print(\"\\nScaler mean (per feature):\", scaler.mean_)\n",
    "print(\"Scaler scale (std per feature):\", scaler.scale_)\n",
    "\n",
    "# Transform training and test data using the learned scaler\n",
    "X_train_scaled = scaler.transform(X_train)\n",
    "X_test_scaled = scaler.transform(X_test)\n",
    "\n",
    "# Print scaled training data\n",
    "print(\"\\nStandardized Training Data (first 5 rows):\")\n",
    "print(X_train_scaled[:5])\n",
    "\n",
    "# Mean and std of the scaled training data (should be approx 0 and 1)\n",
    "print(\"\\nTraining Data - Mean after scaling (per feature):\")\n",
    "print(X_train_scaled.mean(axis=0))\n",
    "\n",
    "print(\"Training Data - Std after scaling (per feature):\")\n",
    "print(X_train_scaled.std(axis=0))\n",
    "\n",
    "# Mean and std of the scaled test data\n",
    "print(\"\\nTest Data - Mean after scaling (per feature):\")\n",
    "print(X_test_scaled.mean(axis=0))\n",
    "\n",
    "print(\"Test Data - Std after scaling (per feature):\")\n",
    "print(X_test_scaled.std(axis=0))\n"

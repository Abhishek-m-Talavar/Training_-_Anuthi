    "# ----------------------------------------\n",
    "# 📦 Step 1: Import Required Libraries\n",
    "# ----------------------------------------\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "from sklearn.preprocessing import StandardScaler\n",
    "from sklearn.model_selection import train_test_split\n",
    "\n",
    "# ----------------------------------------\n",
    "# 📂 Step 2: Load the Dataset\n",
    "# ----------------------------------------\n",
    "diabetes_data = pd.read_csv(\"diabetes.csv\")\n",
    "\n",
    "# View the first 5 rows\n",
    "print(\"\\n🔹 Head of the dataset:\")\n",
    "print(diabetes_data.head())\n",
    "\n",
    "# ----------------------------------------\n",
    "# 🔍 Step 3: Explore Basic Information\n",
    "# ----------------------------------------\n",
    "print(\"\\n🔹 Shape of the dataset (rows, columns):\", diabetes_data.shape)\n",
    "\n",
    "print(\"\\n🔹 Summary statistics:\")\n",
    "print(diabetes_data.describe())\n",
    "\n",
    "# ----------------------------------------\n",
    "# 🎯 Step 4: Separate Features and Target\n",
    "# ----------------------------------------\n",
    "X = diabetes_data.drop(columns='Outcome', axis=1)  # Features\n",
    "Y = diabetes_data['Outcome']                      # Target\n",
    "\n",
    "# ----------------------------------------\n",
    "# 📊 Step 5: Standardize the Feature Data\n",
    "# ----------------------------------------\n",
    "scaler = StandardScaler()\n",
    "standardized_data = scaler.fit_transform(X)\n",
    "\n",
    "# View the standardized data (as a NumPy array)\n",
    "print(\"\\n🔹 Standardized feature data (first 5 rows):\")\n",
    "print(standardized_data[:5])\n",
    "\n",
    "# Replace X with the standardized version\n",
    "X = standardized_data\n",
    "\n",
    "# ----------------------------------------\n",
    "# 🔀 Step 6: Split Data into Train and Test Sets\n",
    "# ----------------------------------------\n",
    "X_train, X_test, Y_train, Y_test = train_test_split(X, Y, test_size=0.2, random_state=2)\n",
    "\n",
    "# ----------------------------------------\n",
    "# 📏 Step 7: Print Final Shapes\n",
    "# ----------------------------------------\n",
    "print(\"\\n✅ Final shapes:\")\n",
    "print(\"Full X shape:\", X.shape)\n",
    "print(\"X_train shape:\", X_train.shape)\n",
    "print(\"X_test shape:\", X_test.shape)\n"
   ]
